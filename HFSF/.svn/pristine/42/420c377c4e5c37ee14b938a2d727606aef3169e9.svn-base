package com.sinc.hfsf.admin.vo;

public class AdminVO {
	/*
	private int orderSum; // 총 주문건수
	private int nowPacking; // 팩킹중
	private int finishPacking;
	private int finishTransfer;
	*/
	
	private String ord_id; // 주문코드
	private String pickup_id; // 픽업대코드
	private String customer_id; // 고객코드
	private String odr_date;
	private String odr_state;
	private String total_price;
	// 토큰은 필요없을듯.
	public AdminVO() {
		super();
		// TODO Auto-generated constructor stub
	}
	public AdminVO(String ord_id, String pickup_id, String customer_id, String odr_date, String odr_state,
			String total_price) {
		super();
		this.ord_id = ord_id;
		this.pickup_id = pickup_id;
		this.customer_id = customer_id;
		this.odr_date = odr_date;
		this.odr_state = odr_state;
		this.total_price = total_price;
	}
	public String getOrd_id() {
		return ord_id;
	}
	public void setOrd_id(String ord_id) {
		this.ord_id = ord_id;
	}
	public String getPickup_id() {
		return pickup_id;
	}
	public void setPickup_id(String pickup_id) {
		this.pickup_id = pickup_id;
	}
	public String getCustomer_id() {
		return customer_id;
	}
	public void setCustomer_id(String customer_id) {
		this.customer_id = customer_id;
	}
	public String getOdr_date() {
		return odr_date;
	}
	public void setOdr_date(String odr_date) {
		this.odr_date = odr_date;
	}
	public String getOdr_state() {
		return odr_state;
	}
	public void setOdr_state(String odr_state) {
		this.odr_state = odr_state;
	}
	public String getTotal_price() {
		return total_price;
	}
	public void setTotal_price(String total_price) {
		this.total_price = total_price;
	}


	
	
	
}
