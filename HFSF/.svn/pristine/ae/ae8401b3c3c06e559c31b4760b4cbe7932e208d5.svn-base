package com.sinc.hfsf.qr.vo;

public class QrVO {
	
	private String odr_id;
	private String pickup_id;
	private String customer_id;
	private String odr_date;
	private String odr_state;
	private int total_price;
	
	public QrVO() {
		super();
	}

	public QrVO(String odr_id, String pickup_id, String customer_id, String odr_date, String odr_state,
			int total_price) {
		super();
		this.odr_id = odr_id;
		this.pickup_id = pickup_id;
		this.customer_id = customer_id;
		this.odr_date = odr_date;
		this.odr_state = odr_state;
		this.total_price = total_price;
	}

	public String getOdr_id() {
		return odr_id;
	}

	public void setOdr_id(String odr_id) {
		this.odr_id = odr_id;
	}

	public String getPickup_id() {
		return pickup_id;
	}

	public void setPickup_id(String pickup_id) {
		this.pickup_id = pickup_id;
	}

	public String getCustomer_id() {
		return customer_id;
	}

	public void setCustomer_id(String customer_id) {
		this.customer_id = customer_id;
	}

	public String getOdr_date() {
		return odr_date;
	}

	public void setOdr_date(String odr_date) {
		this.odr_date = odr_date;
	}

	public String getOdr_state() {
		return odr_state;
	}

	public void setOdr_state(String odr_state) {
		this.odr_state = odr_state;
	}

	public int getTotal_price() {
		return total_price;
	}

	public void setTotal_price(int total_price) {
		this.total_price = total_price;
	}
	
	
}
